@page "/"

<PageTitle>Statybinių prekių parduotuvė</PageTitle>
<body id="page-top">
<!-- Masthead-->
<header class="masthead">

    <div class="container">
        <div class="row justify-content-center">
            <div class="text-center">
                <h1 class="section-heading fw-bold bg-light">Statybinių prekių parduotuvė</h1>
            </div>

            <div class="col-9">
                <img class="img-fluid"
                     src="https://images.unsplash.com/photo-1482731215275-a1f151646268?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8M
                        HxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1470&q=80"/>
            </div>
        </div>
    </div>
</header>
<!-- Services-->
<section class="page-section mt-5" id="services">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading bg-light">Mūsų nuostabioje parduotuvėje galite</h2>
        </div>
        <div class="row text-center mt-2">
            <div class="col-md-4">
                <img class="img-fluid" height="110px" src="https://cdn-icons-png.flaticon.com/512/2420/2420157.png"
                     width="110px"/>
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fas fa-shopping-cart fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="my-3">Pirkti</h4>
                <p class="text-muted">Neįtikėtinai žemos kainos</p>
            </div>
            <div class="col-md-4">
                <img class="img-fluid" height="110px" src="https://cdn-icons-png.flaticon.com/512/679/679720.png"
                     width="110px"/>
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fas fa-laptop fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="my-3">Gauti siuntą į paštomatą</h4>
                <p class="text-muted">Greitas pristatymas (iki 2 d.d.)</p>
            </div>
            <div class="col-md-4">
                <img class="img-fluid" height="110px" src="https://cdn-icons-png.flaticon.com/512/782/782899.png"
                     width="110px"/>
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fas fa-lock fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="my-3">Dirbti</h4>
                <p class="text-muted">Galite įsidarbinti mūsų įmonėje</p>
            </div>
        </div>
    </div>
</section>
<!-- Team-->
<section class="page-section bg-light mt-5" id="team">
    <div class="container">
        <div class="text-center justify-content-center">
            <h2 class="section-heading">Mūsų komanda</h2>
        </div>
        <div class="row">
            <div class="col-lg-3">
                <div class="team-member">
                    <h4>Šefas</h4>
                    <p class="text-muted">Chef</p>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="team-member">
                    <h4>Robke</h4>
                    <p class="text-muted">Noobas</p>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="team-member">
                    <h4>Dreineris konteineris</h4>
                    <p class="text-muted">Lyderis</p>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="team-member">
                    <h4>A7</h4>
                    <p class="text-muted">Galvosūkis</p>
                </div>
            </div>
        </div>
    </div>
</section>
</body>
