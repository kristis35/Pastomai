@page "/"

<PageTitle>Index</PageTitle>
<body id="page-top">      
        <!-- Masthead-->
        <header class="masthead">
        
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-6">
                    <img class="img-fluid" src="https://images.unsplash.com/photo-1482731215275-a1f151646268?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1470&q=80" />
                    </div>
                    <div class="col-4">
                        <h2>Statybų parduotuvė</h2>
                        <div class="masthead-heading">Pas mus geriausios kainos</div>
                        <button type="button" class="btn btn-primary">Parduouvė</button>
                    </div>
                </div>
            </div>
        </header>
        <!-- Services-->
    <section class="page-section mt-5" id="services">
            <div class="container">
                <div class="text-center">
                <h2 class="section-heading bg-light">Ką galite darytį Mūsų nuostabioje parduotuvė</h2>
                </div>
                <div class="row text-center mt-2">
                    <div class="col-md-4">
                    <img class="img-thumbnail" height="110px" width="110px" src="https://cdn-icons-png.flaticon.com/512/2420/2420157.png" />
                        <span class="fa-stack fa-4x">
                            <i class="fas fa-circle fa-stack-2x text-primary"></i>
                            <i class="fas fa-shopping-cart fa-stack-1x fa-inverse"></i>
                        </span>
                        <h4 class="my-3">Pirkti</h4>
                        <p class="text-muted">Nusipirkti geriausiu statybinių prekių</p>
                    </div>
                    <div class="col-md-4">
                    <img class="img-thumbnail" height="110px" width="110px" src="https://cdn-icons-png.flaticon.com/512/679/679720.png" />
                        <span class="fa-stack fa-4x">
                            <i class="fas fa-circle fa-stack-2x text-primary"></i>
                            <i class="fas fa-laptop fa-stack-1x fa-inverse"></i>
                        </span>
                        <h4 class="my-3">Gauti siuntą per paštomatą</h4>
                        <p class="text-muted">Labai greitai net per 2 d.d</p>
                    </div>
                    <div class="col-md-4">
                    <img class="img-thumbnail" height="110px" width="110px" src="https://cdn-icons-png.flaticon.com/512/782/782899.png" />
                        <span class="fa-stack fa-4x">
                            <i class="fas fa-circle fa-stack-2x text-primary"></i>
                            <i class="fas fa-lock fa-stack-1x fa-inverse"></i>
                        </span>
                        <h4 class="my-3">Dirbti</h4>
                        <p class="text-muted">Galite įsidarbinti Mūsų imonėje</p>
                    </div>
                </div>
            </div>
        </section>
        <!-- Team-->
        <section class="page-section bg-light mt-5" id="team">
            <div class="container">
                <div class="text-center">
                    <h2 class="section-heading">Mūsų komanda</h2>
                </div>
                <div class="row">
                    <div class="col-lg-3">
                        <div class="team-member">
                            <h4>Sefas</h4>
                            <p class="text-muted">Chef</p>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="team-member">
                            <h4>Robke</h4>
                            <p class="text-muted">Noobas</p>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="team-member">
                            <h4>Dreineris konteineris</h4>
                            <p class="text-muted">Lyderis</p>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="team-member">
                            <h4>A7</h4>
                            <p class="text-muted">Galvosukis</p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        
        
       
       
       
        
        
